if $(file-exists OMakeroot)
   include MyOCaml
   export

.SUBDIRS: src test

.PHONY: all install clean

# mark file confined directory path.
INSTALLED = $(BIG_ROOT)/installed

setup.ml: _oasis
	oasis setup

LIB = realcaml

GENFILES = $(OCamlPackageGeneratedFiles $(LIB), false)

INSTALL_FILES = $(addprefix src/, $(GENFILES))
INSTALL_FILES += $(ls R, src/*mli)

.DEFAULT: all
